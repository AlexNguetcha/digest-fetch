{
  "name": "digest-fetch",
  "version": "1.0.1",
  "description": "digest auth request plugin for fetch/node-fetch and support http basic auth authentication as well",
  "main": "digest-fetch-src.js",
  "scripts": {
    "lint": "eslint --plugin markdown --ext js,md .",
    "test": "mocha --check-leaks --bail --no-exit --reporter spec test/digest-*",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --check-leaks --no-exit --reporter dot test/",
    "test-travis": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --check-leaks --no-exit --reporter spec test/",
    "build": "NODE_ENV=\"production\" webpack --optimize-minimize"
  },
  "keywords": [
    "digest",
    "auth",
    "fetch",
    "node-feth",
    "http",
    "basic",
    "authentication"
  ],
  "author": "stefanliu@outlook.com",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devfans/digest-fetch.git"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "after": "^0.8.2",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "eslint": "^5.9.0",
    "eslint-plugin-markdown": "^1.0.0-rc.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "node-fetch": "^2.3.0",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "express": "^4.17.1",
    "passport": "^0.4.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "passport-http": "^0.3.0"
  },
  "homepage": "https://github.com/devfans/digest-fetch#readme",
  "dependencies": {
    "base-64": "^0.1.0",
    "crypto-js": "^3.1.9-1"
  }
}
